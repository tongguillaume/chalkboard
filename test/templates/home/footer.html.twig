    <!-- Logout Modal-->
    {% if traduction is defined %}
        <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">{{ traduction.pret_a_partir |trans}}</h5>
                        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">Ã—</span>
                        </button>
                    </div>
                    <div class="modal-body">{{ traduction.selectionnez_deconnexion_ci_dessous_si_vous_etes_pret_a_terminer_votre_session_en_cours | trans}}</div>
                    <div class="modal-footer">
                        <button class="btn btn-secondary" type="button" data-dismiss="modal">{{ traduction.annuler | trans}}</button>
                        <a class="btn btn-primary" href="{{ app.request.getBaseURL()  }}/logout" onclick='localStorage.setItem("sideBar", "true")'>{{ traduction.deconnexion | trans }}</a>
                    </div>
                </div>
            </div>
        </div>
    {% endif %}

